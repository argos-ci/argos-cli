{
  "name": "argos-cli",
  "version": "0.0.4",
  "description": "Argos command line interface.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argos-ci/argos-cli.git"
  },
  "license": "MIT",
  "bin": {
    "argos": "bin/argos"
  },
  "keywords": [
    "visual regression",
    "diff-ui",
    "compare-screenshots",
    "review-process"
  ],
  "scripts": {
    "dev": "babel --watch -d lib src",
    "build": "NODE_ENV=production babel -d lib src",
    "prepublish": "npm run build",
    "test": "eslint . && jest --runInBand"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-stage-1": "^6.22.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-find-rules": "^1.14.3",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-react": "^6.10.0",
    "jest": "^19.0.2",
    "sinon": "^2.1.0"
  },
  "dependencies": {
    "convict": "^2.0.0",
    "form-data": "^2.1.2",
    "klaw": "^1.3.1",
    "mz": "^2.6.0",
    "node-fetch": "^1.6.3",
    "raven": "^1.2.0",
    "through2": "^2.0.3",
    "update-notifier": "^2.1.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "<rootDir>/test/setupEnv.js"
    ]
  }
}
